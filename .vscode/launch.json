{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Zoroaster",
      "program": "${workspaceFolder}/node_modules/.bin/zoroaster",
      "env": {
        "NODE_DEBUG": "@zoroaster/mask",
        "ZOROASTER_INTERACTIVE": "1",
      },
      "console": "integratedTerminal",
      "args": [
        "test/spec",
        "test/mask",
        "-a",
        "-w",
        "-t",
        "9999999"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Requirements",
      "program": "${workspaceFolder}/node_modules/.bin/zoroaster",
      "console": "integratedTerminal",
      "args": [
        "test/requirements/get-results/default.js",
        "-a",
        "-w",
        "-t",
        "9999999"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch For Package",
      "program": "${workspaceFolder}/node_modules/.bin/zoroaster",
      "console": "integratedTerminal",
      "args": [
        "test/mask", "-i",
        "-a", "-w", "-t", "9999999"
      ],
      "cwd": "${workspaceFolder}/../../adc/documentary",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Example",
      "program": "${workspaceFolder}/example/index.js",
      "env": {
        "NODE_DEBUG": "@zoroaster/mask",
      },
      "console": "integratedTerminal",
      "args": [
        "example/example.js"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    }
  ]
}
