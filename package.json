{
  "name": "@zoroaster/mask",
  "version": "1.6.0",
  "description": "The Code For Zoroaster Mask Testing.",
  "main": "build/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc -o README.md -n _contextTesting",
    "depack": "GOOGLE_CLOSURE_COMPILER=/Volumes/backup/closure-compiler/target/closure-compiler-1.0-SNAPSHOT.jar depack depack/depack.js -l -a -p -o depack/index.js --externs externs.js -s",
    "b": "alamode src -o build -s",
    "d": "yarn-s d1",
    "d1": "typal src/index.js -c",
    "externs": "typal externs.js -e",
    "build": "yarn-s d b doc",
    "e": "alanode",
    "example/": "yarn e example/example.js"
  },
  "files": [
    "build",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/contexttesting/mask.git"
  },
  "keywords": [
    "mask",
    "spec",
    "zoroaster",
    "test",
    "context",
    "testing"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/contexttesting/mask/issues"
  },
  "homepage": "https://github.com/contexttesting/mask#readme",
  "devDependencies": {
    "alamode": "^2.0.0",
    "documentary": "^1.26.2",
    "eslint-config-artdeco": "1.0.1",
    "reloquent": "^1.2.4",
    "temp-context": "^2.1.3",
    "yarn-s": "1.1.0",
    "zoroaster": "^3.13.0"
  },
  "dependencies": {
    "@zoroaster/deep-equal": "^1.0.1",
    "@zoroaster/fork": "^1.2.3",
    "assert-throws": "3.2.1",
    "catchment": "^3.3.0",
    "erte": "^1.2.0",
    "mismatch": "^1.2.0"
  }
}
