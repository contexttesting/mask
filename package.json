{
  "name": "@zoroaster/mask",
  "version": "2.2.0",
  "description": "The Code For Zoroaster Mask Testing.",
  "main": "depack/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test-compile": "ALAMODE_ENV=test-compile yarn test",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc -o README.md -n _contextTesting",
    "compile": "depack-dev src/depack.js -a -p -o compile -s -O 2018 -S",
    "build": "yarn-s d b doc",
    "e": "alanode",
    "example/": "yarn e example/example.js",
    "d": "yarn-s d1 externs types",
    "types": "typal types/index.js -u",
    "d1": "typal compile/index.js -u -t types/index.xml",
    "externs": "typal types/externs.js -e"
  },
  "files": [
    "compile",
    "types/index.js",
    "types/externs.js"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/contexttesting/mask.git"
  },
  "keywords": [
    "mask",
    "spec",
    "zoroaster",
    "test",
    "context",
    "testing"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/contexttesting/mask/issues"
  },
  "homepage": "https://github.com/contexttesting/mask#readme",
  "devDependencies": {
    "@zoroaster/deep-equal": "^1.0.1",
    "@zoroaster/fork": "^1.3.2",
    "alamode": "^3.0.0",
    "assert-throws": "^3.3.0",
    "catchment": "^3.3.0",
    "documentary": "^1.32.2",
    "erte": "^1.2.0",
    "eslint-config-artdeco": "1.0.1",
    "mismatch": "^1.2.0",
    "reloquent": "^1.3.2",
    "temp-context": "^2.1.3",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.2"
  }
}
